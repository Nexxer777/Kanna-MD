{
  "name": "Vibranium-Bot",
  "version": "1.0.1",
  "description": "Vibranium-Bot, A simple light Weight Bot",
  "logo": "https://i.ibb.co/x8yyTq2/20240112-010232.png",
  "keywords": ["whatsapp", "bot", "md", "multi-device", "Vibranium", "Vibranium-MD", "wabot", "termux-bot", "sc-bot"],
  "repository": "https://github.com/V-E-N-O-X/Vibranium-Bot",
  "stack": "container",
  "env": {
    "AUTO_READ": {
      "description": "It will seen all incoming dm msgs if true",
      "required": false
      "value": "false"
    },
    "ALWAYS_ONLINE": {
       "description": "True or false"
       "required": false
       "value": "true"
     },
     "ANTICALL_MODE": {
       "description": "true if u dont wanna get any imcoming calls"
       "required": false
       "value": "false"
     },
    "MONGO_DB": {
      "description": "Put your mongodb url here",
      "required": false,
    },
    "HEROKU_APP_NAME": {
      "description": "Enter Your Heroku App Name Same As Above You Entered",
      "required": true,
    },
    "HEROKU_API_KEY": {
      "description": "Enter Your Heroku Account Api Key. Get It From https://dashboard.heroku.com/account"
      "required": true,
    },
    "OWNER_NAME": {
      "description": "Put bot owner name",
      "required": false,
      "value": "Vᴇɴᴏx"
    },
    "SUDO": {
      "description": "Enter Your Owner number",
      "required": false,
      "value": "8801975492880"
    },
    "PREFIX": {
      "description": "Enter Your Bot prefix",
      "required": true,
      "value": "."
    },
    "PUBLIC_MODE": {
      "description": "true if you wanna keep public else false if private",
      "required": false,
      "value": "true"
    }
  },
    "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/DuckyTeam/heroku-buildpack-imagemagick.git"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
    }
  ]
}