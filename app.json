{
  "name": "Vibranium-Bot",
  "description": "Simple lightweight Whatsapp Bot",
  "keywords": [
    "whatsapp bot"
  ],
  "repository": "https://github.com/V-E-N-O-X/Vibranium-Bot",
  "stack": "container",
  "env": {
       "HAPP": {
      "description": "Put the Heroku app name, same as above entered",
      "required": true,
      "value": ""
    },
       "HKEY": {
      "description": "Put your Heroku api key Here , Get one from here https://dashboard.heroku.com/account",
      "required": true,
      "value": ""
    },
    "DATABASE_URL": {
      "description": "paste your mongodb database url",
      "required": true,
      "value": ""
    },
    "SESSION_ID": {
      "description": "paste your session id",
      "required": true,
      "value": ""
    },
    "REMOVEBG_KEY": {
      "description": "paste your RemoveBg api key",
      "required": false,
      "value": ""
    },
    "MODE": {
      "description": "set it to public or private",
      "required": true,
      "value": "public"
    },
    "PREFIX": {
      "description": "put any one symbol here except @ and + , leave it Blank if you want to use multiple prefix",
      "required": false,
      "value": ""
    },
    "statusview": {
      "description": "make it true if you want bot to view status",
      "required": false,
      "value": "false"
    },
    "autoRead": {
      "description": "make it true if you want bot to read messages",
      "required": false,
      "value": "true"
    },
    "antidelete": {
      "description": "bot will forward deleted messages if you make it true",
      "required": false,
      "value": "false"
    }
    },
    "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/DuckyTeam/heroku-buildpack-imagemagick.git"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
    }
  ]
}

